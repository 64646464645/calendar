
.content {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
		padding-top: 100px;
}
.logo {
		height: 200rpx;
		width: 200rpx;
		margin-left: auto;
		margin-right: auto;
		margin-bottom: 50rpx;
}
.text-area {
		display: flex;
		justify-content: center;
		margin-bottom: 50px;
}
.title {
		font-size: 36rpx;
		color: #8f8f94;
}
.navigate-btn {
		width: 200px;
		height: 40px;
		background-color: #007AFF;
		color: white;
		border: none;
		border-radius: 20px;
}
